
year = int(input("Which year do you want to check? "))

year4 = year % 4
year100 = year % 100
year400 = year % 400
if year4 >0:
  print (f"{year} is not leap year.")
else:
  if year100 >0:
    print (f"{year} is leap year.")
  else:
    if year400 == 0:
      print (f"{year} is leap year.")
    else:
      print (f"{year} is not leap year.")
